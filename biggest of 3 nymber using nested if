#include <stdio.h>

int main()
{
    int a,b,c;
    printf("enter 3 number: \n");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c)
        printf("A is the biggest number\n");
        else if(b>c)
        printf("B is the bigeest number\n");
        else
        printf("C is the bigeest number\n");
    return 0;
}
