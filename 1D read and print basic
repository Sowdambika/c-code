#include <stdio.h>

int main()
{
    int x[10],n,i;
    
    printf("enter number of elements:");
    scanf("%d",&n);
    
    printf(" enter %d array elements:",n);
    for(i=0;i<n;i++)
        scanf("%d",&x[i]);
    
    printf("array elements are:\n");
    for(i=0;i<n;i++)
        printf("x[%d] = %d\n",i,x[i]);

    return 0;
}
