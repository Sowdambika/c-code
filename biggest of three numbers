#include <stdio.h>

int main()
{
    int x,y,z;
    
    printf("Enter 3 numbers: ");
    scanf("%d%d%d",&x,&y,&z);
    
    if(x>y && x>z)
    printf("x is the biggest number %d\n",x);
    else if(y>x && y>z)
    printf("y is the biggest number %d\n",y);
    else
    printf("z is the biggest number %d\n",z);

    return 0;
}
