#include <stdio.h>

int main()
{
    int j,i,n,mid;
    printf("enter n value:");
    scanf("%d",&n);
    mid=(n/2)+1;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            if((i+j)%2==0)
            printf(" + ");
            else
            printf(" - ");
        }
        printf("\n");
    }
    return 0;
}
