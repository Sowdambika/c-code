#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>

int main()
{
    char alpha;
    printf("enter the alphabet: \n");
    scanf("%c",&alpha);
    
    alpha=tolower(alpha);
    switch(alpha)
    {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        printf("Vowel");
        break;

        default:
        printf("Consonant");
        
    }
    return 0;
}
