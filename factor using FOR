#include<stdio.h>
#include<math.h>

int main()
{
    int n,count;
    printf("enter number value:");
    scanf("%d",&n);
    count=0;
    
    for(int i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            printf("%d\n",i);
            count++;
        }
    }
    printf("number of factors are: %d",count);
    
}
